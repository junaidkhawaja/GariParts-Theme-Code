<div class="{{ section.id }} hero_banner_cq">
  <div class="banner_cq">
    <div class="banner_container_cq">
      {% for block in section.blocks %}
        <div
          class="banner_slide_box"
          style="background-image: url('{{ block.settings.background_image | img_url: 'master' }}');"
        >
          <a href="{{ block.settings.whole_banner_link }}">
            <div
              class="banner_slide_box_cont"
              style="background-color:{{ block.settings.slide_box_back_color }}99; text-align: {{ block.settings.overlay_text_align }}; justify-content: {{ block.settings.text_container }};"
            >
              <div
                class="text_contain"
                style="text-align: {{ block.settings.overlay_text_align }}; justify-content: {{ block.settings.text_container }};"
              >
                <h2>{{ block.settings.heading }}</h2>
                <h3>{{ block.settings.para }}</h3>
                {% if block.settings.btn_text != blank %}
                  <a href="{{ block.settings.btn_link }}">
                    {{ block.settings.btn_text }}
                  </a>
                {% endif %}
              </div>
            </div>
          </a>
        </div>
      {% endfor %}
    </div>
  </div>
</div>

<style>
    .hero_banner_cq{
      width: 100%;
    }
    .banner_slide_box{
      background-position: 50% 50%;
      background-repeat: no-repeat;
      background-size: cover;
      height: 100%;
      display: flex !important;
      flex-direction: column;
      justify-content: center;
    }
    .banner_container_cq{
      height: 100%;
    }
    .banner_slide_box_cont h2 p{
      font-size: {{ section.settings.heading_fz }}px;
      font-weight: {{section.settings.heading_fw}};
      color: {{section.settings.heading_fc}};
      font-family: 'Inter', sans-serif;
      line-height: normal;
      margin-bottom: {{ section.settings.mar_bo_text }}px;
          letter-spacing: 0px;
    }
    .banner_slide_box_cont h3 p{
      font-size: {{ section.settings.para_fz }}px;
      font-weight: {{section.settings.para_fw}};
      color: {{section.settings.para_fc}};
      font-family: 'Inter', sans-serif;
      line-height: normal;
      margin-bottom: {{ section.settings.mar_bo_text }}px;
      letter-spacing: 0px;
    }
    .banner_slide_box_cont a{
      font-size: {{ section.settings.btn_fz }}px;
      font-weight: {{section.settings.btn_fw}};
      color: {{section.settings.btn_fc}};
      background-color: {{section.settings.btn_fc_bg}};
      padding: 10px 30px;
      line-height: normal;
      border: 3px solid #F68E1E;
      font-family: 'Inter', sans-serif;
    }
     .banner_slide_box_cont a:hover{
      background-color: {{section.settings.btn_fc}};
    color: #fff;
    }
    .text_contain{
      padding:{{ section.settings.slide_box_pad_tb }}px {{ section.settings.slide_box_pad_lr }}px;
      width: 100%;
      max-width: 660px;
      display: flex;
      flex-wrap: wrap;
      padding-bottom: 30px;
    }
    .banner_slide_box_cont{
      width: 100%;
      height: 600px;
      display: flex;
      align-items: center;
      padding: 0 40px;
    }
    .{{ section.id }} .slick-dots li button{
      background: #000 !important;
      border-radius: 50%;
      width: 20px !important;
      height: 20px !important;
      border: 2px solid #F68E1E;
    }
    .{{ section.id }} .slick-dots li button:before{
      font-size: 20px !important;
      opacity: 0 !important;
      width: 20px !important;
      height: 20px !important;
      line-height: 22px !important;
    }
    .{{ section.id }} .slick-dots li.slick-active button:before{
      opacity: 1 !important;
      color: #F68E1E;
    }
    .{{ section.id }} .slick-dots{
      bottom: 26px !important;
      display: flex;
      align-items: center;
      justify-content: center;
      column-gap: 20px;
    }
    .{{ section.id }} .slick-dotted.slick-slider {
      margin-bottom: 0 !important;
    }
    .{{ section.id }} .banner_pre_arrow{
      position: absolute;
      z-index: 2;
      top: 50%;
      left: 30px;
      transform: translate(-50%, -50%);
      cursor: pointer;
    }
    .{{ section.id }} .banner_next_arrow{
      position: absolute;
      z-index: 2;
      top: 50%;
      right: -30px;
      transform: translate(-50%, -50%);
      cursor: pointer;
    }

    @media only screen and (max-width:767px){
    .banner_slide_box_cont{
      height: 300px;
    }
    .banner_slide_box_cont {
      padding: 0 20px;
  }
    .banner_slide_box_cont h2 p{
      font-size: {{ section.settings.heading_fz_mb }}px;
    }
    .banner_slide_box_cont h3 p{
      font-size: {{ section.settings.para_fz_mb }}px;
    }
    .text_contain{
      padding:{{ section.settings.slide_box_pad_tb_mb }}px {{ section.settings.slide_box_pad_lr_mb }}px;
      text-align: center !important;
      padding-bottom:0px;
    }
    .banner_slide_box{

    }
    .banner_slide_box_cont h2 p{
      margin-bottom: {{ section.settings.mar_bo_text_mb }}px;
    }
    .banner_slide_box_cont h3 p{
      margin-bottom: {{ section.settings.mar_bo_text_mb }}px;
    }
    }
     #bannerImage[aria-disabled="true"] {
              display:none !important;
          }
    #bannerImage[aria-disabled="false"] {
              display: block !important;
          }

    @media only screen and (max-width: 600px){
      .banner_slide_box_cont{
        height: 200px;
      }
    }
</style>
<script type="text/javascript">
  $(document).ready(function() {
  $('.banner_container_cq').slick({
    slidesToShow: 1,
   slidesToScroll: 1,
   infinite: false,
   autoplay: {{ section.settings.slick_autoplay }},
   autoplaySpeed: {{ section.settings.slick_autoplay_speed }},
    dots: {{ section.settings.slick_dots }},
    arrows: {{ section.settings.slick_arrow }},
    centerPadding: '60px',
    {% if section.settings.pre_arr != blank %}
    prevArrow:'<img class="banner_pre_arrow" id="bannerImage" src="{{ section.settings.pre_arr | img_url: 'master' }}"/>',
    {% endif %}
    {% if section.settings.next_arr != blank %}
    nextArrow:'<img class="banner_next_arrow" id="bannerImage" src="{{ section.settings.next_arr | img_url: 'master' }}"/>',
    {% endif %}
    responsive: [
      {
       breakpoint: 1200,
       settings: {
         arrows: false,
         centerMode: false,
         centerPadding: '40px',
         slidesToShow: 1,
         slidesToScroll: 1,
       }
     },
     {
       breakpoint: 990,
       settings: {
         arrows: false,
         centerMode: false,
         centerPadding: '40px',
         slidesToShow: 1,
         slidesToScroll: 1,
       }
     },
     {
       breakpoint: 768,
       settings: {
         arrows: false,
         centerMode: false,
         centerPadding: '40px',
         slidesToShow: 1,
         slidesToScroll: 1,
       }
     },
     {
       breakpoint: 600,
       settings: {
         arrows: false,
         centerMode: false,
         centerPadding: '40px',
         slidesToShow: 1,
         slidesToScroll: 1,
       }
     }
   ]
  });
  });
</script>

