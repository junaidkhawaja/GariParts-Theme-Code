<script>
     $(function() {
           // (Optional) Active an item if it has the class "is-active"
           $(".accordion > .accordion-item.is-active").children(".accordion-panel").slideDown();

    	$(".accordion > .accordion-item").click(function() {
           // Cancel the siblings
           $(this).siblings(".accordion-item").removeClass("is-active").children(".accordion-panel").slideUp();
       // Toggle the item
       $(this).toggleClass("is-active").children(".accordion-panel").slideToggle("ease-out");
    	});
    });

    const faqBlockHead = document.querySelectorAll('.heading_list');
    const faqBlockAcc = document.querySelectorAll('.accordion_block_list');
    faqBlockAcc[0].classList.add('block_show');
    faqBlockHead[0].classList.add('head_button_style');


    faqBlockHead.forEach((e, i) => {
        e.addEventListener('click', () => {
            faqBlockAcc.forEach((j, index) => {
                j.classList.remove('block_show');
            });
            faqBlockHead.forEach((h) => {
                h.classList.remove('head_button_style'); // Remove the 'clicked' class from all headings
            });
            faqBlockAcc[i].classList.add('block_show'); // Add 'block_show' class to corresponding .accordion_block_list
            e.classList.add('head_button_style'); // Add 'clicked' class to the clicked heading
        });
    });


    {

      const readMoreReadLess = (daddyDivSelector) => {
          const daddyDiv = daddyDivSelector;
          const readMore = daddyDiv.querySelector('.acc_more');
          const readLess = daddyDiv.querySelector('.acc_less');
          const allParas = daddyDiv.querySelector('.accordion-rich-text').children;

          function showMessages(cnt, [...elements]) {
              elements.forEach((ele) => {
                  ele.classList.remove('hide');
                  readMore.style.display = "none"
              })
          }

          function hideMessages(cnt, [...elements]) {
              elements.forEach((ele, ind) => {
                  cnt != ind ? ele.classList.add('hide') : ele.classList.remove('hide');
                  readLess.style.display = "none"
              })
          }

          let count = readMore.dataset.count;
          for (let i = count; i < allParas.length; i++) {
              allParas[i].classList.add('hide');
          }

          let allParasExcept_1st = daddyDivSelector.querySelectorAll('.accordion-rich-text p:not(:first-child)');
          allParasExcept_1st.forEach((p) => {
              p.classList.add('hide')
          })

          let allParasP = daddyDivSelector.querySelectorAll('.accordion-rich-text p');
          if(allParasP.length  === 1){
            readMore.style.display = 'none'
            readLess.style.display = 'none'
          }

          readMore.addEventListener('click', function (e) {
              e.stopPropagation()
              let counter = readMore.dataset.count || 0;
              readMore.dataset.count = parseInt(counter) + 2;
              readLess.style.display = "block"
              showMessages(counter, allParas);
          });

          readLess.addEventListener('click', function (e) {
              e.stopPropagation()
              let counter = 0;
              readMore.style.display = "block"
              hideMessages(counter, allParas);
          });

      }


      document.querySelectorAll('.accordion').forEach((acc, ind) => {
          acc.querySelectorAll('li').forEach((li) => {
              readMoreReadLess(li);
          })
      })

  }
</script>

