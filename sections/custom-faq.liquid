    background: transparent;
    color: white;
    text-decoration: underline;
    font-size: 16px;
    font-weight: 500;
    padding: 0;
    cursor: pointer;
  }

  #sid-{{ section.id }} .product_inner_div {
    height: 80px;
    overflow: hidden;
  }



  #sid-{{ section.id }} .accordion {
    margin: 0;
    padding: 0;
    list-style: none;
  }

  #sid-{{ section.id }} .accordion-item {
    list-style: none !important;
  }

  #sid-{{ section.id }} .accordion-panel {
    margin: 0;
    padding: 0px 20px 0 0;
    display: none;
  }

  #sid-{{ section.id }} .accordion-panel p {
    color: {{section.settings.para_fc}};
    font-family: 'Poppins', sans-serif;
    font-size: clamp({{ section.settings.para_fz_mb }}px, ({{section.settings.para_fz}} / 1440) * 100vw, {{ section.settings.para_fz }}px);
    font-style: normal;
    font-weight: {{section.settings.para_fw}};
    line-height: normal;

  }

  #sid-{{ section.id }} .accordion-item.is-active .accordion-thumb::before {
    transform: rotate(45deg);
  }


  #sid-{{ section.id }} .accordion-panel .page-content {
    padding-top: 10px;
    padding-bottom: 10px;
  }

  #sid-{{ section.id }} .accordion-panel .page-width {
    padding: 0px 0px
  }

  #sid-{{ section.id }} .accordion-thumb {
    font-weight: {{section.settings.heading_fw}};
    font-size: clamp({{section.settings.heading_fz_mb}}px, ({{section.settings.heading_fz}} / 1440) * 100vw, {{section.settings.heading_fz}}px);
