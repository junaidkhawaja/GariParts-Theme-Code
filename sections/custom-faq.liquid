    font-family: 'Poppins', sans-serif;
    font-style: normal;
    line-height: 1;
    letter-spacing: 0.116px;
    padding: 12px 0;
  }

  #sid-{{ section.id }} .accordion-thumb {
    display: flex;
    flex-direction: row-reverse;
    justify-content: space-between;
    border-bottom: 1px solid #fff;
    align-items: center;
  }

  #sid-{{ section.id }} .acc_minus {
    transition: transform 0.2s ease-out;
    display: none;
    width: 25px;
    cursor: pointer;
  }

  #sid-{{ section.id }} .acc_plus {
    transition: transform 0.2s ease-out;
    display: block;
    width: 25px;
    cursor: pointer;
  }

  #sid-{{ section.id }} .accordion .is-active .acc_minus {
    display: block;
  }

  #sid-{{ section.id }} .accordion .is-active .acc_plus {
    display: none;
  }

  #sid-{{ section.id }} .acc_more,
  #sid-{{ section.id }} .acc_less {
    border: none;
    background: transparent;
    color: #ffffff;
    text-decoration: underline;
    font-size: 16px;
    font-weight: 500;
    padding: 0;
    cursor: pointer;
  }

  #sid-{{ section.id }} .acc_less {
    display: none;
  }

  #sid-{{ section.id }} .accordion-panel_des p {
    height: 50px;
    overflow: hidden;
  }

  @media only screen and (max-width:767px) {
  #sid-{{ section.id }} .product_inner_div {
      height: 60px;
  }

  #sid-{{ section.id }} .read_less_btn,
  #sid-{{ section.id }} .read_more_btn {
      font-size: 14px;
  }
  }
</style>

